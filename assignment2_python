   "source": [
    "for n in range(1,101):\n",
    "    if n % 3 == 0 and n % 5 != 0:\n",
    "        print(\"Fizz: \", n)\n",
    "    elif n % 3 == 0 and  n % 5 == 0:\n",
    "        print(\"FizzBuzz: \", n)\n",
    "    elif n % 5 == 0 and n % 7 != 0:\n",
    "        print(\"Buzz: \", n)\n",
    "    elif n % 7 == 0 and n % 5 != 0:\n",
    "        print(\"Woof: \", n)\n",
    "    elif n % 5 == 0 and n % 7 == 0:\n",
    "        print(\"BuzzWoof: \", n)\n",
    "    else:\n",
    "        print(n, \" is not divisible by 3, 5, or 7.\")"
   ]
